{
  "env": {
    "browser": true,
    "es2022": true,
    "mocha": true,
    "node": true
  },
  "plugins": ["@typescript-eslint", "import"],
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "import/order": "warn",
    "no-use-before-define": "off"
  }
}
